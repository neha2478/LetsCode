//Using functions
//Using a method we can do...
public class Main
{
	public static void main(String[] args) {
	    
		int r = 5;
        partternStars(r); //calling the methods...
		
	}
	
	
	static void partternStars(int row){
	    
	    for(int i = 0 ; i <= row ; i++){
	        for(int j = 0 ; j <= i ; j++){
	            System.out.print("* ");
	        }
	        System.out.println();
	    }
	}
}

Task --- //To Print reverse parttern...

//Using a method we can do...
public class Main
{
	public static void main(String[] args) {
	     int r = 5;
       	     partternStars(r); //calling the methods...
		
	}
	
	//for reverse parttern
	static void partternStars(int row){
	//Logic of the code....
	    int i, j;
	    for(i = 0 ; i <= row ; i++){
	        
	        // inner loop to handle number spaces
            // values changing acc. to requirement
	        for(j = 2 * (row - i); j >= 0 ; j--){
	            
	            System.out.print(" ");
	        }
	        
	        //  inner loop to handle number of columns
            //  values changing acc. to outer loop
	        for(j = i ; j >= 0 ; j--){
	            
	            System.out.print("* ");
	        }
	        System.out.println();
	    }
	}
}

Task 36> To CALCULATE THE AVERAGE OF THE ARRAY....
class A{
    public static void main(String[] args){
        //To Calculate Average using Arrays...
        //here we have created an araay... of size double...
        int[] arr = {23, 45, 67, 89, 100, 120, 236};
        int sum = 0;
        
        //this is one way of using for loop
        // for(double num : numArray){
        //     sum += num;
        // }
        
        //normal way of using for loop
        for(int i = 0 ; i <= arr.length - 1 ; i++){
            sum = sum + i;
        }
        
        double average = sum /arr.length;
        
        System.out.println("The average of the array is : "+ average);
    }
}

/*
O/P:-
The average of the array is : 3.0
*/

Task 37> Program to FIND THE LARGEST ELEMENT OF THE ARRAY....
class A{
    public static void main(String[] args){
        //to find the largest element in the Array...
        int arr[] = {12, 34, 56, 78, 89, 100};
        int largest_ele = arr[0]; // we initialized that with arr[0] the ekment...
        
        for(int i = 0 ; i <= arr.length - 1; i++){
            if(largest_ele < arr[i]){
                largest_ele = arr[i];
            }
        }
        
        System.out.println("The largest element is :" + largest_ele);
    }
}

//Another way using the for loop...
class A{
    public static void main(String[] args){
        //to find the largest element in the Array...
        int arr[] = {12, 34, 56, 78, 89, 100};
        int largest_ele = arr[0]; // we initialized that with arr[0] the ekment...

	//generally tis we used in Array list and collections...
        for (int num : arr){
            if(largest_ele < num){
                largest_ele = num ;
            }
        }
        
        System.out.println("The largest element is :" + largest_ele);
    }
}
/*
Output:- 
The largest element is : 100*/


Task 38> TO ADD TWO MATRIX USING MULTI DIMENSIONAL ARRAY....
class Main{
    public static void main(String[] args){
       //To Add two Matrix Using Multi-dimensional Array...
       
       int rows = 2, columns = 3;
       
       int[][] arr1 = { {2, 3, 4}, {4, 5, 6} };
       int[][] arr2 = { {3, 4, 5}, {7, 8, 9} };
       
       int[][] sum = new int[rows][columns]; //here passing the rows and columns..
       
        //For loop to accelerate on the matrix/3 dimensional array..
        //i for rows and j is for columns
        for(int i = 0 ; i < rows; i++){ 
            for(int j = 0 ; j < columns; j++){
                sum[i][j] = arr1[i][j] + arr2[i][j];
            }
        }
       
        System.out.println("The sum of the two matrices is : ");
        
        for(int[] row : sum){
            for(int column : row){
                System.out.println(column + "   ");
            }
            System.out.println();
        }
        
    }
}


Task 39> TO MULTIPLY TWO MATRIX USING MULTI DIMENSIONAL ARRAY....
class Multiply_Matrix{
    public static void main(String[] args){
       //To Multiply two Matrix Using Multi-dimensional Array...
       
       int r1 = 2, c1 = 2;
       int r2 = 3, c2 =2;
       
       int arr1[][] = { {2,3} , {4, 5} };
       int arr2[][] = { {7,8} , {8, 10} , {4,7}};
       
       int prod[][] = new int[r1][c2];
       
       for(int i = 0 ; i < r1; i++){
           for(int j = 0 ; j < c2; j++){
               for(int k = 0; k <c1 ; k++)
               prod[i][j] += arr1[i][j] * arr2[i][j];
           }
       }
       
       System.out.println("The product of two matrices is : ");
       
        for(int[] row : prod){
            for(int column : row){
                System.out.print(column + "  ");
            }
            System.out.println();
        }
    }
}
/*
The product of two matrices is :
28   48
64  100
*/

Task 40> To TRANSOSE THE MATRIX
class Transpose_Matrix{
    public static void main(String[] args){
       //Transpose of amatrix...
       int rows = 3, columns = 3;
       int[][] matrix = { {2,3,4}, {4,5,6}, {7,8,9} };
       
       display(matrix);//calling the display() method..
       
       //to transpose the matrix...
       int[][] transpose = new int[columns][rows]; // store in reverse columns than rows columns in first and than rows..
       
       //outer loop for rows
       //the i should be 0 <= i < rows
       for(int i = 0 ; i< rows; i++){
           //inner loop for columns
	   //the j should be 0 <= j < columns
           for(int j = 0 ; j < columns; j++){
               transpose[j][i] = matrix[i][j];
           }
       }
       
       display(transpose);
        
    }
    
    static void display(int[][] matrix){
        System.out.println("The matrix is :");
	//here k to iterate on  the rows
        for(int[] k : matrix){
	    //here l to iterate on  the columns
            for(int l: k){
                System.out.print(l + " ");
            }
            System.out.println();
        }
    }
}


Task 41> To print the number of charcters in the string..
class Main{
    public static void main (String[] args) {
        //No of charcter in string / alternate no of sapces only we need to == instead of !=.
        
        String str = "This Website is aewsome.";
        
        int f = 0 ; //frequency - count the no of characters in the string..
        
        for(int i = 0 ; i <= str.length() - 1; i++){
            if(str.charAt(i) != ' '){
                f++;
            }
        }
        System.out.println("The no of chracters in the string is: " + f);
    }
}


Task 42> To COUNT the FREQUENCY of CHARCTER in String.
class Count_Freq{
    
    public static void main (String[] args) {
        //Find the frequency of a character in the string....
       
       String str =  "Hi my Name is Neha";
       char ch = 'a'; //the charcter we want to search...
       int f = 0; //counter variable...
       
       //to iterate over the array...
        for(int i = 0 ; i <= str.length() - 1; i++){
            //str.charAt(i) - is to fetch the charcter at particular index in string...
            if( ch == str.charAt(i) ){
                f++; //it it is found than f++
            }     
        }
        
        System.out.println("The frequency of the "+ ch + "is :" + f);
    }
}

Task 43> COUNT the No of VOWELS, CONSONENTS, DIGITS , SPACES in the string....
class Main{
    
    public static void main (String[] args) {
       //the no of digits , spaces, vowels, consonents in the string are..
       String str = "The website is 56824@ aewsome0.";
       int v = 0, c = 0, d = 0, s = 0;
       //converting into lower case... 
       str = str.toLowerCase();
       
        for(int i = 0 ; i <= str.length()-1; i++){
           
            char ch = str.charAt(i); //storing the chracter into the ch to check.. the conditions..
           
            //conditions to check vowels..
            if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u'){
                ++v;
            }
           
            //conditions to check consonents..
            else if((ch >= 'a' && ch <= 'z')){
                ++c;
            }
            
            //to count digits..
            else if(ch >= '0' && ch <= '9'){
                ++d;
            }
            
            //else if count spaces..
            else if(ch == ' '){
                ++s;
            }
           
        }
       System.out.println("Vowels :" + v);
       System.out.println("Consonents :" + c);
       System.out.println("Digits :" + d);
       System.out.println("Spaces :" + s);
    }
}


