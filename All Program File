    public static void main(String args[]){
       int fir = 1234;
       int sec = 3456;
       
       int sum = fir + sec;
       
       System.out.println("The sum is : "+ sum);
    }
}
//Output:-
//The sum is : 4690

Task 3> Java Program to multipy two floating Numbers...
class MultiplyFloatingpoints{
    public static void main(String args[]){
        //in java the data type for float we using f literal... which is compulsory..
       float first = 1.5f;
       
       float second = 2.0f;
       
       float prod = first + second;
       
       System.out.println("The product is: " + prod);
    }
}


Task 4>Java program to find ASCII values of chracters...
class AsciiValue{
    public static void main(String args[]){
        char ch = 'a';
        int ascii = ch;
        int castAscii = (int) ch; //here we have used type casting 
        
        System.out.print("The ASCII value of "+ ch + " is: " + ascii );
        System.out.println();
        System.out.println("The ASCII value of "+ ch + " is: " + castAscii );
    }
}

Task 5>  Java Programm  to compute Quotient and Reminder...
class Quotien_And_Reminder{
    public static void main(String args[]){
        int a = 25 , b = 45;
        
        int quotient = a / b;
        int reminder = a % b;
        
        System.out.println("The Quotient of a and b = "+ quotient);
        System.out.println("The Reminder of a nad b = " + reminder);
    }
}

//Output:-
The Quotient of a and b = 0
The Reminder of a nad b = 25


Task 6> Java Programm to swap two Numbers..(with 3rd variable / without 3rd variable..)
//with 3rd variable...
class SwaptwoNumbers_ThirdVar{
    public static void main(String[] args){
        int a = 34, b = 45;
        
        //Who the look before swaping...
        System.out.println("--Before Swaping---");
        System.out.println("a = " + a);
        System.out.println("b = " + b);
        
        //Now the logic is...
        int temp = a;
        a = b;
        b = temp;
        
        //Who the look after swaping...
        System.out.println("--After Swaping---");
        System.out.println("a = " + a);
        System.out.println("b = " + b);
    }
}
/*
Output:- 
--Before Swaping---
a = 34
b = 45
--After Swaping---
a = 45
b = 34
*/

//without 3rd variable...
class SwaptwoNumbers_ThirdVar{
    public static void main(String[] args){
        int a = 34, b = 45;
        
        //Who the look before swaping...
        System.out.println("--Before Swaping---");
        System.out.println("a = " + a);
        System.out.println("b = " + b);
        
        //Now the logic is...
        a = a + b;
        b = a - b;
        a = a - b;
        
        //Who the look after swaping...
        System.out.println("--After Swaping---");
        System.out.println("a = " + a);
        System.out.println("b = " + b);
    }
}

//Another logic for the code...
/*Now the logic is...
        a = a ^ b;
        b = a ^ b;
        a = a ^ b;
*/
//Here we will get same output only...
/*
Output:- 
--Before Swaping---
a = 34
b = 45
--After Swaping---
a = 45
b = 34
